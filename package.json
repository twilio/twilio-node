{
  "name": "twilio",
  "description": "A Twilio helper library",
  "version": "3.4.0",
  "author": "API Team <api@twilio.com>",
  "contributors": [
    {
      "name": "twilio-node contributors",
      "url": "https://github.com/twilio/twilio-node/graphs/contributors"
    }
  ],
  "keywords": [
    "twilio",
    "sms",
    "rest",
    "api"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/twilio/twilio-node.git"
  },
  "dependencies": {
    "jsonwebtoken": "^7.4.1",
    "lodash": "4.0.0",
    "moment": "2.18.1",
    "q": "2.0.x",
    "request": "2.81.x",
    "rootpath": "0.1.2",
    "scmp": "0.0.3",
    "xmlbuilder": "8.2.2",
    "deprecate": "1.0.0"
  },
  "devDependencies": {
    "express": "3.x",
    "jasmine-node": "1.14.5",
    "jshint": "2.8.x",
    "jscs": "3.0.7",
    "jsdoc": "3.4.x"
  },
  "scripts": {
    "test": "jasmine-node --captureExceptions spec",
    "jshint": "jshint lib/rest/** lib/base/** lib/http/**",
    "jscs": "jscs -c .jscsrc lib/base/** lib/http/** --fix",
    "check": "npm run jshint; npm run jscs",
    "jsdoc": "jsdoc -r lib -d docs"
  },
  "files": [
    "lib",
    "index.js"
  ],
  "main": "./lib",
  "engines": {
    "node": ">=0.12.0"
  },
  "license": "MIT"
}
